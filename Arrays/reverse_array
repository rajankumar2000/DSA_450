// #define ll long long int 
// #define fio ios::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL)
using namespace std;

void solve(){
  int n;
  cin>>n;

int v[n];

for(int i=0;i<n;i++){
	cin>>v[i];
}


for(int i=n-1;i>=0;i--){
	cout<<v[i]<<" ";
}
cout<<endl;

}

int main(){
    int t;
  cin>>t;
    while(t--){
        solve();
    }
    return 0;
}
